{
	"name": "block-navigation",
	"version": "3.4.0",
	"description": "Block Navigation",
	"homepage": "https://wordpress.org/plugins/block-navigation",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"prebuild": "npm run test:types && npm run clean:dist && npm run clean:release",
		"build": "webpack --mode=production",
		"postbuild": "node scripts/replace-version.js && node scripts/copy-files.js",
		"clean:dist": "rm -rf dist",
		"clean:release": "rm -rf _release",
		"dev": "webpack --mode=development",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier",
		"test:types": "tsc",
		"test:lint": "eslint \"**/*.{js,ts,tsx}\"",
		"test:prettier": "prettier --check \"**/*.{js,ts,tsx,php,json}\"",
		"prettier": "prettier --write \"**/*.{js,ts,tsx,php,json}\""
	},
	"dependencies": {
		"@babel/core": "^7.13.10",
		"@babel/preset-env": "^7.13.10",
		"@babel/preset-react": "^7.12.13",
		"@babel/preset-typescript": "^7.13.0",
		"@babel/register": "^7.13.8",
		"@types/lodash": "^4.14.168",
		"@types/react-window": "^1.8.2",
		"@types/wordpress__block-editor": "^2.2.9",
		"@types/wordpress__blocks": "^6.4.12",
		"@types/wordpress__edit-post": "^3.5.4",
		"@types/wordpress__plugins": "^2.3.7",
		"@wordpress/browserslist-config": "^3.0.1",
		"@wordpress/components": "^12.0.8",
		"@wordpress/data": "^4.26.8",
		"@wordpress/element": "^2.19.1",
		"@wordpress/hooks": "^2.11.1",
		"@wordpress/i18n": "^3.18.0",
		"@wordpress/rich-text": "^3.24.8",
		"babel-loader": "^8.2.2",
		"browserslist": "^4.16.5",
		"copy-text-to-clipboard": "^3.0.1",
		"copyfiles": "^2.4.1",
		"css-loader": "^5.1.2",
		"css-minimizer-webpack-plugin": "^1.2.0",
		"mini-css-extract-plugin": "^1.3.9",
		"nib": "^1.1.2",
		"react-tiny-popover": "^6.0.5",
		"react-window": "^1.8.6",
		"replace-in-file": "^6.2.0",
		"stylus": "^0.54.8",
		"stylus-loader": "^5.0.0",
		"terser-webpack-plugin": "^5.1.1",
		"typescript": "^4.2.3",
		"webpack": "^5.24.4",
		"webpack-cli": "^4.5.0"
	},
	"devDependencies": {
		"@prettier/plugin-php": "^0.16.1",
		"@typescript-eslint/eslint-plugin": "^4.17.0",
		"@typescript-eslint/parser": "^4.17.0",
		"eslint": "^7.21.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.22.0",
		"prettier": "^2.2.1"
	}
}
