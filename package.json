{
	"name": "block-navigation",
	"version": "4.1.1",
	"description": "Block Navigation",
	"homepage": "https://wordpress.org/plugins/block-navigation",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"prebuild": "npm run test:types && npm run clean:dist && npm run clean:release",
		"build": "webpack --mode=production",
		"postbuild": "node scripts/replace-version.js && node scripts/copy-files.js",
		"clean:dist": "rm -rf dist",
		"clean:release": "rm -rf _release",
		"dev": "webpack --mode=development",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier && npm run test:unit",
		"test:types": "tsc",
		"test:lint": "eslint \"**/*.{js,ts,tsx}\"",
		"test:prettier": "prettier --check \"**/*.{js,ts,tsx,php,json}\"",
		"prettier": "prettier --write \"**/*.{js,ts,tsx,php,json}\"",
		"test:unit": "jest",
		"test:unit:watch": "jest --watch"
	},
	"dependencies": {
		"@babel/core": "^7.16.12",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"@babel/preset-typescript": "^7.16.7",
		"@babel/register": "^7.16.9",
		"@types/lodash": "^4.14.178",
		"@types/wordpress__block-editor": "^6.0.5",
		"@types/wordpress__components": "^14.0.10",
		"@types/wordpress__data": "^4.6.10",
		"@types/wordpress__edit-post": "^4.0.1",
		"@types/wordpress__plugins": "^3.0.0",
		"@types/wordpress__rich-text": "^3.4.6",
		"@wordpress/blocks": "^11.1.5",
		"@wordpress/browserslist-config": "^4.1.0",
		"@wordpress/data": "^6.1.5",
		"@wordpress/element": "^4.0.4",
		"@wordpress/hooks": "^3.2.2",
		"@wordpress/i18n": "^4.2.4",
		"babel-loader": "^8.2.3",
		"browserslist": "^4.19.1",
		"copy-text-to-clipboard": "^3.0.1",
		"copyfiles": "^2.4.1",
		"css-loader": "^6.5.1",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"mini-css-extract-plugin": "^2.5.2",
		"nib": "^1.1.2",
		"react-tiny-popover": "^7.0.1",
		"replace-in-file": "^6.3.2",
		"stylus": "^0.56.0",
		"stylus-loader": "^6.2.0",
		"terser-webpack-plugin": "^5.3.0",
		"typescript": "^4.5.5",
		"webpack": "^5.76.0",
		"webpack-cli": "^4.9.2"
	},
	"devDependencies": {
		"@prettier/plugin-php": "^0.17.6",
		"@types/jest": "^27.4.0",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^8.7.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-webpack": "^0.13.2",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jest": "^26.0.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^27.4.7",
		"prettier": "^2.5.1"
	}
}
