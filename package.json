{
	"name": "block-navigation",
	"version": "1.2.0",
	"description": "Block Navigation WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/block-navigation/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"dev": "webpack --mode development --watch --config ./.webpack/build.config.babel.js",
		"prod": "webpack --mode production --config ./.webpack/build.config.babel.js",
		"copy": "webpack --mode production --config ./.webpack/copy.config.babel.js && rm _extras/release/_temp.js",
		"version": "webpack --mode production --config ./.webpack/version.config.babel.js && rm _temp.js",
		"release": "rm -rf build _extras/release && npm run type && npm run version && npm run prod && npm run copy",
		"type": "tsc"
	},
	"dependencies": {
		"copy-text-to-clipboard": "^2.1.0",
		"react-tiny-popover": "^3.4.5"
	},
	"devDependencies": {
		"@babel/core": "^7.5.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/register": "^7.5.5",
		"@types/lodash": "^4.14.138",
		"@types/node": "^12.7.3",
		"@types/wordpress__block-editor": "^2.2.2",
		"@types/wordpress__blocks": "^6.4.3",
		"@types/wordpress__components": "^7.4.4",
		"@types/wordpress__compose": "^3.4.0",
		"@types/wordpress__data": "^4.6.2",
		"@types/wordpress__edit-post": "^3.5.0",
		"@types/wordpress__element": "^2.4.0",
		"@types/wordpress__hooks": "^2.4.0",
		"@types/wordpress__i18n": "^3.4.0",
		"@types/wordpress__plugins": "^2.3.3",
		"@types/wordpress__rich-text": "^3.4.2",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.4",
		"css-loader": "^3.2.0",
		"file-loader": "^4.2.0",
		"mini-css-extract-plugin": "^0.8.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"string-replace-loader": "^2.2.0",
		"style-loader": "^1.0.0",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
