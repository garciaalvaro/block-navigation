{
	"name": "block-navigation",
	"version": "4.0.1",
	"description": "Block Navigation",
	"homepage": "https://wordpress.org/plugins/block-navigation",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"prebuild": "npm run test:types && npm run clean:dist && npm run clean:release",
		"build": "webpack --mode=production",
		"postbuild": "node scripts/replace-version.js && node scripts/copy-files.js",
		"clean:dist": "rm -rf dist",
		"clean:release": "rm -rf _release",
		"dev": "webpack --mode=development",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier && npm run test:unit",
		"test:types": "tsc",
		"test:lint": "eslint \"**/*.{js,ts,tsx}\"",
		"test:prettier": "prettier --check \"**/*.{js,ts,tsx,php,json}\"",
		"prettier": "prettier --write \"**/*.{js,ts,tsx,php,json}\"",
		"test:unit": "jest",
		"test:unit:watch": "jest --watch"
	},
	"dependencies": {
		"@babel/core": "^7.15.0",
		"@babel/preset-env": "^7.15.0",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.15.0",
		"@babel/register": "^7.15.3",
		"@types/lodash": "^4.14.172",
		"@types/wordpress__block-editor": "^6.0.3",
		"@types/wordpress__components": "^14.0.3",
		"@types/wordpress__data": "^4.6.10",
		"@types/wordpress__edit-post": "^4.0.1",
		"@types/wordpress__plugins": "^3.0.0",
		"@types/wordpress__rich-text": "^3.4.6",
		"@wordpress/blocks": "^11.0.0",
		"@wordpress/browserslist-config": "^4.1.0",
		"@wordpress/data": "^6.0.0",
		"@wordpress/element": "^4.0.0",
		"@wordpress/hooks": "^3.2.0",
		"@wordpress/i18n": "^4.2.1",
		"babel-loader": "^8.2.2",
		"browserslist": "^4.16.8",
		"copy-text-to-clipboard": "^3.0.1",
		"copyfiles": "^2.4.1",
		"css-loader": "^6.2.0",
		"css-minimizer-webpack-plugin": "^3.0.2",
		"mini-css-extract-plugin": "^2.2.0",
		"nib": "^1.1.2",
		"react-tiny-popover": "^6.0.10",
		"replace-in-file": "^6.2.0",
		"stylus": "^0.54.8",
		"stylus-loader": "^6.1.0",
		"terser-webpack-plugin": "^5.1.4",
		"typescript": "^4.3.5",
		"webpack": "^5.50.0",
		"webpack-cli": "^4.8.0"
	},
	"devDependencies": {
		"@prettier/plugin-php": "^0.17.3",
		"@types/jest": "^27.0.1",
		"@typescript-eslint/eslint-plugin": "^4.29.2",
		"@typescript-eslint/parser": "^4.29.2",
		"eslint": "^7.32.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-webpack": "^0.13.1",
		"eslint-plugin-import": "^2.24.0",
		"eslint-plugin-jest": "^24.4.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^27.0.6",
		"prettier": "^2.3.2"
	}
}
