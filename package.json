{
	"name": "block-navigation",
	"plugin_name": "Block Navigation",
	"version": "1.1.2",
	"description": "Block Navigation",
	"plugin_uri": "https://wordpress.org/plugins/block-navigation/",
	"main": "index.js",
	"scripts": {
		"dev": "webpack --mode development --watch --config ./.webpack/src.config.babel.js",
		"prod": "webpack --mode production --config ./.webpack/src.config.babel.js",
		"copy": "webpack --mode production --config ./.webpack/release-copy.config.js",
		"replace": "webpack --mode production --config ./.webpack/release-replace.config.js",
		"remove": "rm _extras/release/_replace.js _extras/release/_copy.js",
		"release": "npm run prod && npm run copy && npm run replace && npm run remove",
		"type": "tsc"
	},
